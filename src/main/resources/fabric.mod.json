{
  "schemaVersion": 1,
  "id": "hold_my_plushies",
  "version": "${version}",
  "name": "Hold My Plushies",
  "description": "A very simple and small mod which makes plushies from all sorts of library mods look better when being held.",
  "authors": [
    {
      "name": "LilBroCodes",
      "contact": {
        "email": "lilbrocodes@gmail.com"
      }
    }
  ],
  "contact": {},
  "license": "CC-BY-NC-SA 2.0",
  "icon": "assets/hold_my_plushies/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "org.lilbrocodes.hold_my_plushies.client.HoldMyPlushiesClient"
    ],
    "main": [
      "org.lilbrocodes.hold_my_plushies.HoldMyPlushies"
    ]
  },
  "mixins": [
    "hold_my_plushies.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
